<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>beforeRunningCommand</key>
	<string>saveModifiedFiles</string>
	<key>command</key>
	<string>JAVADOC=${TM_JAVADOC:-"/System/Library/Frameworks/JavaVM.framework/Versions/CurrentJDK/Home/docs/index.html"}
if [[ -f $JAVADOC ]]
   then echo "&lt;meta http-equiv=\"refresh\" content=\"0; tm-file://$JAVADOC\"&gt;"
   else echo "&lt;p&gt;Couldn't find Java documentation.&lt;p&gt;You may need to set &lt;tt&gt;TM_JAVADOC&lt;/tt&gt; to the location of the root documentation file."
fi</string>
	<key>input</key>
	<string>none</string>
	<key>keyEquivalent</key>
	<string>^h</string>
	<key>name</key>
	<string>Documentation</string>
	<key>output</key>
	<string>showAsHTML</string>
	<key>scope</key>
	<string>source.scala</string>
	<key>uuid</key>
	<string>CE007C8C-9A0B-43B8-AEAF-45613BCD3DF7</string>
</dict>
</plist>
